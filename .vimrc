"Turn on line numbers
set number
"Turn on filetype based indent
filetype indent on
"Don't redraw during macros
set lazyredraw
"Highlight matching brackets
set showmatch
"set cursorline
set cursorline
"enable syntax processing
syntax enable
"set backup directory as /tmp
set backup
set backupdir=~/.vim-tmp,~/.tmp,~/tmp,/var/tmp,/tmp
set backupskip=/tmp/*,/private/tmp/*
set directory=~/.vim-tmp,~/.tmp,~/tmp,/var/tmp,/tmp
set writebackup
set background=dark
set t_Co=256
"Change colorscheme
"colorscheme badwolf
colorscheme molokai
"colorscheme goodwolf 

